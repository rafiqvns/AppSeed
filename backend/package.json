    "redux-promise-middleware": "^6.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.29.1"
  },
  "prettier": {
    "useTabs": false,
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "printWidth": 120,
    "semi": true,
    "tabWidth": 2
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix --config frontend/config/.eslintrc --ignore-path frontend/config/.eslintignore",
      "prettier --write"
    ],
    "*.{html,scss,css}": [
      "stylelint --fix --config .stylelintrc",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
