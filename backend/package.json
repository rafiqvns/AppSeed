{
  "name": "certified-safe-driver",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./frontend/config/webpack/webpack.dev.js",
    "build": "rm -rf ./build && webpack -p --config ./frontend/config/webpack/webpack.prod.js",
    "test": "jest -c jest.config.js",
    "test:update": "jest -c jest.config.js --updateSnapshot --silent",
    "lint": "eslint --config ./.eslintrc frontend",
    "lint:fix": "eslint --fix frontend",
    "format": "prettier --write \"frontend/**/*.+(js|jsx|json|css|scss|md)\"",
    "fe-format": "npm run lint && npm run format"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "4.2.2",
    "@testing-library/react": "9.3.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "3.2.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "fibers": "4.0.2",
    "file-loader": "^4.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "^25.1.0",
    "lint-staged": "9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.12.0",
    "sass": "1.23.2",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "superstatic": "^6.0.4",
    "terser-webpack-plugin": "^2.2.1",
    "thread-loader": "2.1.3",
    "url-loader": "^2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.8.0",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.19.2",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "date-fns": "^2.0.0-beta.5",
    "dotenv-webpack": "^1.7.0",
    "formik": "^2.1.4",
    "google-map-react": "^1.1.7",
    "history": "4.10.1",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.1.1",
    "lodash": "^4.17.20",
    "material-table": "^1.63.1",
    "moment": "^2.24.0",
    "normalize.css": "8.0.1",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-alert": "^7.0.0",
    "react-chartist": "0.14.3",
    "react-dom": "^16.12.0",
    "react-easy-print": "^0.6.0",
    "react-google-charts": "^3.0.15",
    "react-hot-loader": "4.12.15",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-spring": "^8.0.27",
    "react-swipeable-views": "0.13.9",
    "react-to-print": "^2.10.0",
    "react-toastify": "^6.1.0",
